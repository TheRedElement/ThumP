{
    "$schema": {
        "objectId": [
            "string. ",
            "id of the object to be displayed above each cell in the thumbnail mosaic. ",
            "each 'objectId' can have a different number of thumbnails (app will automatically adjust). "
        ],
        "link": [
            "string. ",
            "link to some other webpage related to the object. ",
            "will be set as `href` for `objectId. ",
            "for example link to a online database that has additional information (i.e., fink -- ztf.fink-portal.org). "
        ],
        "comments": [
            "list. ",
            "each list entry is one unique comment. ",
            "additional comments you might have. ",
            "i.e.: alternate IDs. "
        ],
        "thumbnailTypes": [
            "List[string]. ",
            "list of the type each thumbnail in 'thumbnails' represents. ",
            "displayed as tooltip in the application. "
        ],
        "thumbnails": [
            "list[array[array]]. ",
            "list of 2d-arrays. ",
            "each cell defines a single pixel in an image. ",
            "the contained value in each cell defines it's brightness. ",
            "each thumbnail can have different dimensions (number of pixels) (app will adjust automatically). "
        ]
    },
